{
  "name": "skill-dev-proxy",
  "description": "Proxy skill for Alice for testing other skills",
  "version": "1.1.1",
  "main": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "init": "git config core.hooksPath ./.hooks",
    "lint": "eslint src test --ext .ts && tsc --noEmit",
    "test:log": "mocha -r ts-node/register test/globals.ts test/specs/*.ts",
    "test": "LOG_LEVEL=fatal npm run test:log",
    "test:dist": "npm run build && HANDLER_PATH=../dist npm test",
    "debug:amqp": "ts-node test/debug-amqp",
    "build": "rm -rf dist && tsc",
    "release": "npm run lint && npm run test:dist && npm version $VER && npm publish && git push --follow-tags --no-verify",
    "release-patch": "VER=patch npm run release",
    "release-minor": "VER=minor npm run release"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": "eslint --ext .ts"
  },
  "dependencies": {
    "alice-renderer": "^1.1.0",
    "amqplib": "^0.7.1",
    "await-timeout": "^1.1.1",
    "console-log-level": "^1.4.1",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@types/amqplib": "^0.5.17",
    "@types/chai": "^4.2.17",
    "@types/console-log-level": "^1.4.0",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.0.1",
    "@types/node-fetch": "^2.5.10",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "alice-tester": "^2.1.1",
    "alice-types": "^1.0.1",
    "chai": "^4.3.4",
    "dotenv": "^8.2.0",
    "eslint": "^7.24.0",
    "eslint-plugin-mocha": "^8.1.0",
    "lint-staged": "^10.5.4",
    "mocha": "^8.3.2",
    "nock": "^13.0.11",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
}
