{
  "name": "skill-dev-proxy",
  "description": "Proxy skill for testing other skills",
  "version": "2.0.5",
  "main": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "init": "git config core.hooksPath ./.hooks",
    "lint": "eslint src test --ext .ts",
    "test:l": "mocha -r ts-node/register test/setup.ts test/specs/*.ts",
    "test": "LOG_LEVEL=fatal npm run test:l",
    "build": "rm -rf dist && tsc",
    "deploy:heroku": "git push heroku",
    "release": "npm run lint && npm t && npm run build && npm version $VER && npm publish && git push --follow-tags --no-verify",
    "release-patch": "VER=patch npm run release",
    "release-minor": "VER=minor npm run release"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": "eslint --ext .ts"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "abort-controller": "^3.0.0",
    "alice-renderer": "^1.1.0",
    "await-timeout": "^1.1.1",
    "console-log-level": "^1.4.1",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "node-fetch": "^2.6.1",
    "websocket": "^1.0.34"
  },
  "devDependencies": {
    "@types/chai": "^4.2.17",
    "@types/console-log-level": "^1.4.0",
    "@types/express": "^4.17.13",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.0.1",
    "@types/node-fetch": "^2.5.10",
    "@types/sinon": "^10.0.2",
    "@types/websocket": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "alice-tester": "^2.1.1",
    "alice-types": "^1.0.1",
    "chai": "^4.3.4",
    "eslint": "^7.24.0",
    "eslint-plugin-mocha": "^8.1.0",
    "get-port": "^5.1.1",
    "lint-staged": "^10.5.4",
    "mocha": "^8.3.2",
    "nock": "^13.0.11",
    "sinon": "^11.1.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "author": {
    "name": "Vitaliy Potapov"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/vitalets/skill-dev-proxy.git"
  },
  "license": "MIT"
}
